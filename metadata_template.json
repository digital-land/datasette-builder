{
  "title": "S3 Parquet Files",
  "description": "Datasette instance for querying Parquet files from an S3 bucket",
  "settings": {  
    "suggest_facets": false
  },
  "plugins": {
    "digital-land-datasette": {
      "issue": {
        "bucket": "$BUCKET_NAME",
        "endpoint_url": "$ENDPOINT_URL",
        "directory": "s3://$BUCKET_NAME/log/issue",
        "httpfs": true
      },
      "expectation": {
        "bucket": "$BUCKET_NAME",
        "endpoint_url": "$ENDPOINT_URL",
        "directory": "s3://$BUCKET_NAME/log/expectation",
        "httpfs": true
      }
    }
  }
}

