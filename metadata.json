{
  "title": "S3 Parquet Files",
  "description": "Datasette instance for querying Parquet files from an S3 bucket",
  "settings": {  
    "suggest_facets": false
  },
  "plugins": {
    "digital-land-datasette": {
      "issue": {
        "bucket": "local-collection-data",
        "endpoint_url": "http://localhost:4566",
        "directory": "s3://local-collection-data/data/issue",
        "httpfs": true
      },
      "expectation": {
        "bucket": "local-collection-data",
        "endpoint_url": "http://localhost:4566",
        "directory": "s3://local-collection-data/data/expectation",
        "httpfs": true
      }
    }
  }
}

